{
  "allowed": [
    "main"
  ],
  "prefixes": [
    "feature",
    "bugfix",
    "hotfix",
    "release"
  ],
  "separator": "/",
  "maxSections": 2,
  "regularExpressions": [
    "^[a-z0-9]{2,}(-[a-z0-9]{1,})*$"
  ],
  "msgSeperatorRequired": "Branch name [%s] must contain a seperator %s.",
  "msgRegexNotMatched": "Branch name [%s] is not allowed. Example: my-feature-1",
  "msgPrefixNotAllowed": "Branch name [%s] is not allowed. Allowed prefix are %s"
}
